{
    "name": "GaGaNFT",
    "version": "0.1.0",
    "manifest_version": 3,
    "description": "GaGaNFT",
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["storage"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
          "16": "/images/icon.png",
          "32": "/images/icon.png",
          "48": "/images/icon.png",
          "128": "/images/icon.png"
        }
    },
    "icons": {
        "16": "/images/icon.png",
        "32": "/images/icon.png",
        "48": "/images/icon.png",
        "128": "/images/icon.png"
      }

}